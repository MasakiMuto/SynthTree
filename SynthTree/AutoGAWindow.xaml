<Window x:Class="SynthTree.AutoGAWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:plot="http://oxyplot.org/wpf"
        Title="AutoGAWindow" Height="600" Width="600">
    <StackPanel>
        <Label x:Name="generation" Content="generation"/>
        <Label x:Name="value" Content="value" />
        <Label x:Name="failCount" Content="failCount" />
        <TextBox x:Name="maxGeneration" Text="100"/>
        <TextBox x:Name="poolSize" Text="50" />
        <Button Content="start" Click="Button_Click" />
        <Button Content="End" Click="EndButtonClick" />
        <Button Content="LoadTree" Click="LoadTreeButtonClick" />
        <Button Content="SelectTarget" Click="SelectTargetButtonClick" />
        <Label x:Name="time" Content="time" />
        <plot:PlotView x:Name="plot" Height="250"/>
    </StackPanel>
</Window>
