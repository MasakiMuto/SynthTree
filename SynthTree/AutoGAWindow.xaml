<Window x:Class="SynthTree.AutoGAWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AutoGAWindow" Height="300" Width="300">
    <StackPanel>
        <Label x:Name="generation" Content="generation"/>
        <Label x:Name="value" Content="value" />
        <Label x:Name="failCount" Content="failCount" />
        <TextBox x:Name="maxGeneration" Text="50"/>
        <TextBox x:Name="poolSize" Text="30" />
        <Button Content="start" Click="Button_Click" />
    </StackPanel>
</Window>
