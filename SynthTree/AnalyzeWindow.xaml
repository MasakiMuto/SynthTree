<Window x:Class="SynthTree.AnalyzeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.org/wpf"
        Title="AnalyzeWindow" Height="600" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <oxy:PlotView x:Name="plot" Grid.Row="0"  Grid.ColumnSpan="2"/>
        <oxy:PlotView x:Name="plot2" Grid.Row="1"  Grid.ColumnSpan="2"/>
        <Button Grid.Column="0" Grid.Row="2" Content="Load" Click="Button_Click" />
    </Grid>
</Window>
